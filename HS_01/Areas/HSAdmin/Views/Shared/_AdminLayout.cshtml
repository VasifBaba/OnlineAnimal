<!DOCTYPE html>
<html lang="en">

<!-- Mirrored from admin.pixelstrap.com/endless/ltr/sample-page.html by HTTrack Website Copier/3.x [XR&CO'2014], Mon, 21 Oct 2019 17:23:05 GMT -->
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="endless admin is super flexible, powerful, clean &amp; modern responsive bootstrap 4 admin template with unlimited possibilities.">
    <meta name="keywords" content="admin template, endless admin template, dashboard template, flat admin template, responsive admin template, web app">
    <meta name="author" content="pixelstrap">
    <link rel="icon" href="~/Areas/HSAdmin/assets/images/favicon.png" type="image/x-icon">
    <link rel="shortcut icon" href="~/Areas/HSAdmin/assets/images/favicon.png" type="image/x-icon">
    <title>Endless - Premium Admin Template</title>
    <!-- Google font-->
    <link href="https://fonts.googleapis.com/css?family=Work+Sans:100,200,300,400,500,600,700,800,900" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <!-- Font Awesome-->
    <link rel="stylesheet" type="text/css" href="~/Areas/HSAdmin/assets/css/fontawesome.css">
    <!-- ico-font-->
    <link rel="stylesheet" type="text/css" href="~/Areas/HSAdmin/assets/css/icofont.css">
    <!-- Themify icon-->
    <link rel="stylesheet" type="text/css" href="~/Areas/HSAdmin/assets/css/themify.css">
    <!-- Flag icon-->
    <!-- Feather icon-->
    <link rel="stylesheet" type="text/css" href="~/Areas/HSAdmin/assets/css/feather-icon.css">
    <!-- Plugins css start-->
    <!-- Plugins css Ends-->
    <!-- Bootstrap css-->
    <link rel="stylesheet" type="text/css" href="~/Areas/HSAdmin/assets/css/bootstrap.css">
    <!-- App css-->
    <link rel="stylesheet" type="text/css" href="~/Areas/HSAdmin/assets/css/style.css">
    <link id="color" rel="stylesheet" href="~/Areas/HSAdmin/assets/css/light-1.css" media="screen">
    <!-- Responsive css-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <link rel="stylesheet" type="text/css" href="~/Areas/HSAdmin/assets/css/responsive.css">
    @RenderSection("head", required: false)

</head>
<body>
    <!-- Loader starts-->
    <div class="loader-wrapper">
        <div class="loader bg-white">
            <div class="whirly-loader"> </div>
        </div>
    </div>
    <!-- Loader ends-->
    <!-- page-wrapper Start-->
    <div class="page-wrapper">
        <!-- Page Header Start-->
        <div class="page-main-header">
            <div class="main-header-right row">
                <div class="main-header-left d-lg-none">
                    <div class="logo-wrapper"><a href="index.html"><img src="~/Areas/HSAdmin/assets/images/endless-logo.png" alt=""></a></div>
                </div>
                <div class="mobile-sidebar d-block">
                    <div class="media-body text-right switch-sm">
                        <label class="switch"><a href="#"><i id="sidebar-toggle" data-feather="align-left"></i></a></label>
                    </div>
                </div>
                <div class="nav-right col p-0">
                    <ul class="nav-menus">
                        <li>
                            <form class="search" action="@Url.Action("Search","Home")" method="post">
                                <div class="input-group w-100">
                                    <input type="text" class="form-control" name="q" placeholder="Axtar">
                                    <div class="input-group-append">
                                        <button class="btn btn-primary" type="submit">
                                            <i class="fa fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </form>
                        </li>
                        <li><a href="@Url.Action("Logout", "AdminAccount")"><i></i>Çıxış</a></li>
                    </ul>
                    <div class="d-lg-none mobile-toggle pull-right"><i data-feather="more-horizontal"></i></div>
                </div>
                <script id="result-template" type="text/x-handlebars-template">
                    <div class="ProfileCard u-cf">
                        <div class="ProfileCard-avatar"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-airplay m-0"><path d="M5 17H4a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h16a2 2 0 0 1 2 2v10a2 2 0 0 1-2 2h-1"></path><polygon points="12 15 17 21 7 21 12 15"></polygon></svg></div>
                        <div class="ProfileCard-details">
                            <div class="ProfileCard-realName">{{name}}</div>
                        </div>
                    </div>
                </script>
                
            </div>
        </div>
        <!-- Page Header Ends-->
        <!-- Page Body Start-->
        <div class="page-body-wrapper">
            <!-- Page Sidebar Start-->
            <div class="page-sidebar">
                <div class="sidebar custom-scrollbar">
                    <ul class="sidebar-menu">
                        <li>
                            <a class="sidebar-header" href="@Url.Action("Index","Home")"><i data-feather="home"></i><span>Əsas səhifə</span><i class="fa fa-angle-right pull-right"></i></a>
                        </li>
                        <li>
                            <a class="sidebar-header" href="@Url.Action("Index","AdminCategories")"><i data-feather="home"></i><span>Kateqoriyalar</span><i class="fa fa-angle-right pull-right"></i></a>
                        </li>
                        <li>
                            <a class="sidebar-header" href="@Url.Action("Index","AdminAnimals")"><i data-feather="home"></i><span>Heyvanlar</span><i class="fa fa-angle-right pull-right"></i></a>
                        </li>
                        <li>
                            <a class="sidebar-header" href="@Url.Action("Index","AdminUsers")"><i data-feather="home"></i><span>İstifadəçilər</span><i class="fa fa-angle-right pull-right"></i></a>
                        </li>
                        <li>
                            <a class="sidebar-header" href="@Url.Action("Index","AdminBanners")"><i data-feather="home"></i><span>Banner</span><i class="fa fa-angle-right pull-right"></i></a>
                        </li>
                        <li>
                            <a class="sidebar-header" href="@Url.Action("Index","Admins")"><i data-feather="home"></i><span>Admin</span><i class="fa fa-angle-right pull-right"></i></a>
                        </li>


                    </ul>
                </div>
            </div>
            <!-- Page Sidebar Ends-->
            <div class="page-body">
                <!-- Container-fluid starts-->
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="card">
                                <div class="card-body mt-5">
                                    @RenderBody()
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Container-fluid Ends-->
            </div>
        </div>
    </div>
    <!-- latest jquery-->
    <script src="~/Areas/HSAdmin/assets/js/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap js-->
    <script src="~/Areas/HSAdmin/assets/js/bootstrap/popper.min.js"></script>
    <script src="~/Areas/HSAdmin/assets/js/bootstrap/bootstrap.js"></script>
    <!-- feather icon js-->
    <script src="~/Areas/HSAdmin/assets/js/icons/feather-icon/feather.min.js"></script>
    <script src="~/Areas/HSAdmin/assets/js/icons/feather-icon/feather-icon.js"></script>
    <!-- Sidebar jquery-->
    <script src="~/Areas/HSAdmin/assets/js/sidebar-menu.js"></script>
    <script src="~/Areas/HSAdmin/assets/js/config.js"></script>
    <!-- Plugins JS start-->
    <script src="~/Areas/HSAdmin/assets/js/typeahead/handlebars.js"></script>
    <script src="~/Areas/HSAdmin/assets/js/typeahead/typeahead.bundle.js"></script>
    <script src="~/Areas/HSAdmin/assets/js/typeahead/typeahead.custom.js"></script>
    <script src="~/Areas/HSAdmin/assets/js/chat-menu.js"></script>
    <script src="~/Areas/HSAdmin/assets/js/tooltip-init.js"></script>
    <script src="~/Areas/HSAdmin/assets/js/typeahead-search/handlebars.js"></script>
    <script src="~/Areas/HSAdmin/assets/js/typeahead-search/typeahead-custom.js"></script>
    <!-- Plugins JS Ends-->
    <!-- Theme js-->
    <script src="~/Areas/HSAdmin/assets/js/script.js"></script>
    <script src="~/Areas/HSAdmin/assets/js/theme-customizer/customizer.js"></script>
    <!-- Plugin used-->
</body>

